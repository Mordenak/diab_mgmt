<h1>Markers#index</h1>


<%= link_to 'New Marker', action: 'new', controller: 'markers' %>

<% if !@markers.empty? %>
  <table>
    <thead>
      <th>Blood Sugar</th>
      <th>Carbs</th>
      <th>Units</th>
      <th>Created At</th>
    </thead>
    <tbody>
        <% @markers.each do |marker| %>
          <tr>
            <% if marker %>
              <td><%= marker.blood_sugar %></td>
              <td><%= marker.carbs %></td>
              <td><%= marker.units %></td>
              <td><%= marker.created_at %></td>
            <% end %>
          </tr>
        <% end %>
    </tbody>
  </table>
<% else  %>
  There are no records at this time.
<% end %>